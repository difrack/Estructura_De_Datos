
-----Memoria Estatica-----

def main():
    calificaciones = []

    for i in range(5):
        calificacion = int(input(f"Captura la calificación {i}: "))
        calificaciones.append(calificacion)

    # Opcional: Mostrar las calificaciones capturadas
    print("\nCalificaciones ingresadas:")
    for i, calificacion in enumerate(calificaciones):
        print(f"Calificación {i}: {calificacion}")

if __name__ == "__main__":
    main()

-----Memoria Dinamica-----

frutas = ["manzana", "mango", "banana", "uvas"]
print("Contenido inicial de la lista:", frutas)

frutas.remove(frutas[0])
frutas += ["sandía"]

print("Lista después de las modificaciones:", frutas)

